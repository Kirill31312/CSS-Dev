local OrionLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/shlexware/Orion/main/source"))()
local Window = OrionLib:MakeWindow({Name = "Select Language!", HidePremium = false, SaveConfig = false, ConfigFolder = "OrionTest"})
local Tab = Window:MakeTab({
 Name = "Menu",
 Icon = "rbxassetid://4483345998",
 PremiumOnly = false
})
Tab:AddButton({
 Name = "English",
 Callback = function()
 loadstring(game:HttpGet('https://raw.githubusercontent.com/Kirill31312/CSS-Dev/main/CSS%20Test%20Dev'))()
 wait(0.5)
 game:GetService("CoreGui").Astereo:Destroy()
 end 
})
Tab:AddButton({
 Name = "Русский (Alpha)",
 Callback = function()
 loadstring(game:HttpGet('https://raw.githubusercontent.com/Kirill31312/CSS-Dev/main/CSSDR'))()
 wait(0.5)
 game:GetService("CoreGui").Astereo:Destroy()
 end 
})
